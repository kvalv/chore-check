{
  "name": "hello-zero-solid",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:ui": "VITE_PUBLIC_SERVER='http://localhost:4848' vite",
    "dev:zero-cache": "zero-cache-dev -p src/schema.ts",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down",
    "dev:clean": "source .env && docker volume rm -f docker_zstart_solid_pgdata && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@rocicorp/zero": "^0.10.2024122404",
    "@solidjs/router": "^0.15.2",
    "@tailwindcss/typography": "^0.5.15",
    "cookie": "^1.0.2",
    "dayjs": "^1.11.13",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "nanoid": "^5.0.9",
    "phosphor-solid": "^1.1.5",
    "postgres": "^3.4.5",
    "solid-collapse": "^1.1.0",
    "solid-js": "^1.9.3",
    "solid-toast": "^0.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241224.0",
    "@eslint/js": "^9.9.0",
    "@hono/node-server": "^1.13.2",
    "@tailwindcss/forms": "^0.5.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.7.9",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "hono": "^4.6.6",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.8"
  }
}
